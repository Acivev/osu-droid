<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <Preference
        android:key="mod_speed_reset"
        android:title="Reset"
        tools:layout="@layout/custom_preference"/>

    <SeekBarPreference
        android:defaultValue="10"
        android:key="mod_speed"
        android:title="Speed modify"
        android:max="30"
        tools:layout="@layout/custom_preference_seekbar" />

    <CheckBoxPreference
        android:defaultValue="false"
        android:key="mod_speed_pitch"
        android:title="Enable pitch shift with speed modify"
        tools:layout="@layout/custom_preference_checkbox" />

</PreferenceScreen>